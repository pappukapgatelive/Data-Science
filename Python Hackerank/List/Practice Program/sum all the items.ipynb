   "source": [
    "import ast\n",
    "\n",
    "mylist = ast.literal_eval(input())\n",
    "\n",
    "list_sum = 0\n",
    "\n",
    "for i in range(0,len(mylist)):\n",
    "    \n",
    "    list_sum += mylist[i]\n",
    "    \n",
    "print(list_sum)"
